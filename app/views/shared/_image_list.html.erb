<div class="view_container">
  <h5>Subimitted Images List</h5>
  <table class="index_table">
    <thead>
    <tr> 
      <th>ID</th>
      <th>Image Name</th> 
    </tr> 
    </thead>
    <tbody>
      <% @image_list.each do |image|%>
        <tr> 
          <td><%= image.id%></td> 
          <td><%= image.image_name%></td> 
        </tr>
      <% end %>
    </tbody>
  </table>
</div>

<style>
.view_container{
  margin: 10px;
}
table {
  font-family: arial, sans-serif;
  border-collapse: collapse;
  width: 100%;
}

td, th {
  border: 1px solid #dddddd;
  text-align: left;
  padding: 8px;
}

</style>